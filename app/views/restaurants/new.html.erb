<h1>Add a new restaurant</h1>
<%= simple_form_for(@restaurant) do |f| %>

  <%= f.input :name %>

  <%= f.input :address %>


  <%= f.input :phone_number %>

  <%= f.select(:category, ["chinese", "italian", "japanese", "french", "belgian"]) %>
  <br><br>
  <button class="btn btn-primary">Create</button>
  <% end %>




